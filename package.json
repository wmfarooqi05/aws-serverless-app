{
  "name": "gel-api",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "sls": "NODE_ENV=local sls ",
    "sls:dev": "NODE_ENV=dev sls ",
    "sls:stage": "NODE_ENV=stage sls ",
    "start": "rm -Rf .esbuild && DEBUG=knex:tx NODE_ENV=local sls offline --reloadHandler",
    "start:dev": "rm -Rf .esbuild && NODE_ENV=dev nodemon --exec sls offline start",
    "start:stage": "rm -Rf .esbuild && NODE_ENV=stage nodemon --exec sls offline start",
    "package:dev": "npm install && NODE_ENV=dev sls package",
    "deploy:dev": "NODE_ENV=dev sls deploy",
    "deploy:stage": "npm install && yarn install && NODE_ENV=stage sls deploy",
    "test": "echo \"Error: no test specified\" && exit 1",
    "knex:migrate:make": "NODE_ENV=local knex --knexfile knex/knex-migrate.ts migrate:make",
    "knex:migrate:latest": "NODE_ENV=local knex --knexfile knex/knex-migrate.ts migrate:latest",
    "knex:migrate:rollback": "NODE_ENV=local knex --knexfile knex/knex-migrate.ts migrate:rollback"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-scheduler": "^3.267.0",
    "@casl/ability": "^6.3.3",
    "@joi/date": "^2.1.0",
    "@middy/core": "^4.2.3",
    "@middy/http-json-body-parser": "^4.2.3",
    "@sendgrid/mail": "^7.7.0",
    "@types/express": "^4.17.17",
    "@vendia/serverless-express": "^4.10.1",
    "aws-serverless-express": "^3.4.0",
    "axios": "^1.3.4",
    "dynamoose": "^3.1.0",
    "express": "^4.18.2",
    "googleapis": "^111.0.0",
    "joi": "^17.7.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^2.4.2",
    "knex-paginate": "^3.0.2",
    "knex-stringcase": "^1.4.6",
    "lodash.clone": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.merge": "^4.6.2",
    "lodash.snakecase": "^4.1.1",
    "middy": "^0.36.0",
    "moment-timezone": "^0.5.40",
    "objection": "^3.0.1",
    "pg": "^8.9.0",
    "redis": "^4.6.4",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.267.0",
    "@aws-sdk/client-dynamodb": "^3.279.0",
    "@aws-sdk/client-elasticache": "^3.272.0",
    "@aws-sdk/client-lambda": "^3.267.0",
    "@aws-sdk/client-s3": "^3.267.0",
    "@aws-sdk/client-ses": "^3.310.0",
    "@aws-sdk/client-sns": "^3.310.0",
    "@aws-sdk/client-sqs": "^3.299.0",
    "@serverless/typescript": "^3.27.0",
    "@types/aws-lambda": "^8.10.110",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash.clone": "^4.5.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/node": "^18.13.0",
    "@types/pg": "^8.6.6",
    "@types/uuid": "^9.0.0",
    "dotenv": "^16.0.3",
    "esbuild": "^0.16.17",
    "esbuild-node-externals": "^1.6.0",
    "esbuild-plugin-lodash": "^1.2.0",
    "json-schema-to-ts": "^2.6.2",
    "nodemon": "^2.0.20",
    "serverless": "^3.27.0",
    "serverless-dotenv-plugin": "^4.0.2",
    "serverless-esbuild": "^1.37.3",
    "serverless-layers": "^2.6.1",
    "serverless-offline": "^12.0.4",
    "serverless-offline-sqs": "^7.3.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT",
  "packageManager": "yarn@3.4.1"
}
